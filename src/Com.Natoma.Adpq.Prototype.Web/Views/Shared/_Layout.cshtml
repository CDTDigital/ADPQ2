<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - com.natoma.hhs.prototype.web</title>
    <base href="/">

    <link rel="stylesheet" href="~/lib/primeng/resources/primeng.css" />
    <link rel="stylesheet" href="~/lib/primeng/resources/themes/bootstrap/theme.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/lib/quill/dist/quill.core.css" />
    <link rel="stylesheet" href="~/lib/quill/dist/quill.snow.css" />

    <link rel="stylesheet" href="~/css/site.css" />

    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <div id="api-url" hidden data-api-url="@ViewData["ApiUrl"]"></div>
    <div class="container body-content">
        @RenderBody()
    </div>

    <script src="~/lib/quill/dist/quill.js"></script>
    <script src="~/lib/chart.js/dist/Chart.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
